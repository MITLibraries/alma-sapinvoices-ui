{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "assigneesFromCodeOwners": true,
  "extends": [
    "schedule:nonOfficeHours"
  ],
  "dependencyDashboard": true,
  "enabledManagers": [
    "pipenv"
  ],
  "lockFileMaintenance": {
    "enabled": true,
    "schedule": [
      "* 0-4 * * 1"
    ]
  },
  "timezone": "America/New_York",
  "vulnerabilityAlerts": {
    "labels": [
      "security"
    ]
  }
}